<%var liked = false
Post.likes.forEach(function(user){
  if(user.toString() == currentUser._id.toString()){
    liked = true
  }
})%>

<%if(liked){%>
  <div class="main-section-item-qty-price-comments-subitem"><span class="like" id="<%=Post._id%>" style="font-weight:bold;">Liked</span> (<span class="number-<%=Post._id%>"><%=Post.likes.length%></span>)</div>
<%} else {%>
  <div class="main-section-item-qty-price-comments-subitem"><span class="dislike" id="<%=Post._id%>">Like</span> (<span class="number-<%=Post._id%>"><%=Post.likes.length%></span>)</div>
<%}%>
